{
  "name": "nodejs-crud",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "npx mikro-orm migration:up && mikro-orm seeder:run && ts-node ./src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migrateProd": "npx mikro-orm migration:up --config=dist/src/mikro-orm.config.js",
    "seedProd": "mikro-orm seeder:run --config=dist/src/mikro-orm.config.js",
    "seed": "mikro-orm seeder:run",
    "test": "jest --forceExit --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.10.0",
    "@mikro-orm/cli": "^6.4.6",
    "@mikro-orm/core": "^6.4.5",
    "@mikro-orm/migrations": "^6.4.5",
    "@mikro-orm/postgresql": "^6.4.5",
    "@mikro-orm/reflection": "^6.4.5",
    "@mikro-orm/seeder": "^6.4.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.1",
    "fastify": "^5.2.1",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "mikro-orm": "^6.4.5",
    "pg": "^8.13.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
